	"item": [
		{
			"name": "Generate access token and refresh token from grant token",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "14215909-c962-44e6-a648-eb826e72afae",
						"exec": [
							"let response = pm.response.json();\r",
							"pm.collectionVariables.set(\"access_token\",response.access_token);\r",
							"pm.collectionVariables.set(\"refresh_token\",response.refresh_token);\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "40ecafdb-f0ec-4417-9fa4-14e2cb6ed8fc",
			"protocolProfileBehavior": {
				"disableBodyPruning": true,
				"disabledSystemHeaders": {
					"user-agent": true,
					"accept": true,
					"accept-encoding": true
				}
			},
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "https://accounts.zoho.com/oauth/v2/token?scope={{scope}}&client_id={{client_id}}&client_secret={{client_secret}}&grant_type=authorization_code&code={{grant_token}}",
					"protocol": "https",
					"host": [
						"accounts",
						"zoho",
						"com"
					],
					"path": [
						"oauth",
						"v2",
						"token"
					],
					"query": [
						{
							"key": "scope",
							"value": "{{scope}}"
						},
						{
							"key": "client_id",
							"value": "{{client_id}}"
						},
						{
							"key": "client_secret",
							"value": "{{client_secret}}"
						},
						{
							"key": "grant_type",
							"value": "authorization_code"
						},
						{
							"key": "code",
							"value": "{{grant_token}}"
						}
					]
				},
				"description": "first go to [https://api-console.zoho.com](https://api-console.zoho.com) and create a self client.\n\nThen generate a grant token. Copy the grant token to params."
			},
			"response": []
		},
		{
			"name": "Generate access token from refresh token",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "f9a01d78-49d9-4827-bf5c-a73cd9fc7bb1",
						"exec": [
							"let response = pm.response.json();\r",
							"pm.collectionVariables.set(\"access_token\",response.access_token);\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"id": "2d63c361-d41c-4651-bdb4-2f457ae5f9aa",
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"method": "POST",
				"header": [],
				"url": {
					"raw": "https://accounts.zoho.com/oauth/v2/token?refresh_token={{refresh_token}}&client_id={{client_id}}&client_secret={{client_secret}}&grant_type=refresh_token",
					"protocol": "https",
					"host": [
						"accounts",
						"zoho",
						"com"
					],
					"path": [
						"oauth",
						"v2",
						"token"
					],
					"query": [
						{
							"key": "refresh_token",
							"value": "{{refresh_token}}",
							"type": "text"
						},
						{
							"key": "client_id",
							"value": "{{client_id}}",
							"type": "text"
						},
						{
							"key": "client_secret",
							"value": "{{client_secret}}",
							"type": "text"
						},
						{
							"key": "grant_type",
							"value": "refresh_token",
							"type": "text"
						}
					]
				}
			},
			"response": []
		},
		{
			"name": "Zoho Analytics export chart data",
			"id": "4e7f4a08-9161-41f5-9888-391c03c3421f",
			"protocolProfileBehavior": {
				"disableBodyPruning": true,
				"disableUrlEncoding": false
			},
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Zoho-oauthtoken {{access_token}}",
						"type": "text"
					},
					{
						"key": "ZANALYTICS-ORGID",
						"value": "{{org_id}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "https://analyticsapi.zoho.com/restapi/v2/workspaces/{{analytics_workspace_id}}/views/{{oka_team_view_id}}/data?CONFIG=%7BresponseFormat%3Aimage%2Cwidth%3A%20764%2Cheight%3A%20373%2Ccriteria%3A%20%22date(%5C%22Timesheet%20(Log%20Hours%20Dashboard)%5C%22.%5C%22Log%20Date%5C%22)%3Ddate(start_date_current_week(now()%2C5))%22%7D",
					"protocol": "https",
					"host": [
						"analyticsapi",
						"zoho",
						"com"
					],
					"path": [
						"restapi",
						"v2",
						"workspaces",
						"{{analytics_workspace_id}}",
						"views",
						"{{oka_team_view_id}}",
						"data"
					],
					"query": [
						{
							"key": "CONFIG",
							"value": "{responseFormat:csv,whatTo:under}",
							"description": "export underlying data",
							"type": "text",
							"disabled": true
						},
						{
							"key": "CONFIG",
							"value": "%7BresponseFormat:image,width:764,height:373%7D",
							"description": "export image",
							"disabled": true
						},
						{
							"key": "CONFIG",
							"value": "%7BresponseFormat%3Aimage%2Cwidth%3A%20764%2Cheight%3A%20373%2Ccriteria%3A%20%22date(%5C%22Timesheet%20(Log%20Hours%20Dashboard)%5C%22.%5C%22Log%20Date%5C%22)%3Ddate(start_date_current_week(now()%2C5))%22%7D",
							"description": "return last thursday"
						}
					]
				}
			},
			"response": []
		},
