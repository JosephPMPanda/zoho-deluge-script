// https://www.zoho.com/crm/developer/docs/api/v2/access-refresh.html

//generate access token
curl -X POST -H "Content-Type: application/x-www-form-urlencoded" -d "grant_type=authorization_code&code={grant token}&client_id={client_id}&client_secret={client_secret}" https://accounts.zoho.com/oauth/v2/token
